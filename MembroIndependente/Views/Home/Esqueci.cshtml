@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>>Cade meu medico? - @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" />
    <style>
        body {
            padding-top: 60px;
        }
    </style>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">
                    Cade meu medico? - Área do Usuário
                </a>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li id="menuMedicos">
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="jumbotron">
            <div class="container">
                <h2>Informe o email cadastrado para continuar</h2>
                <p>&nbsp; </p>
                <div id="status"></div>
                <p>&nbsp; </p>
                <div>
                    <form id="form-login" name="form-login" method="get" action="javascript:function()">
                        <label for="txtEmail">Email</label>&nbsp;
                        <input type="text" id="txtEmail" name="txtEmail" />&nbsp;

                        <a id="botao-enviar" class="btn btn-sm btn-primary"><span class="glyphicon glyphicon-envelope"></span> Enviar</a>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script src="@Url.Content("~/Scripts/jquery-2.1.4.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/Validacao/esqueci.js")"></script>

</body>
</html>
